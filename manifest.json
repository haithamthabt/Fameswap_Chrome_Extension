{
    "manifest_version": 3,
    "name": "Fameswap Scraper",
    "version": "1.0.4",
    "description": "Scrape data from any website.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  